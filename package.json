{
  "name": "postgraphile-simple-starter",
  "version": "1.0.0",
  "description": "Check out the `graphile` folder for installing dependencies and starting up the Postgraphile GraphQL service.",
  "main": "index.js",
  "scripts": {
    "server": "cd graphile && npm run dev",
    "codegen": "cd codegen && npm run codegen",
    "client": "cd client && npm run start",
    "dev": "concurrently \"npm:server\" \"npm:codegen\" \"npm:client\""
  },
